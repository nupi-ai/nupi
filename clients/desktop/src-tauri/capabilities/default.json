{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-set-fullscreen",
    "core:window:allow-set-simple-fullscreen",
    "shell:allow-spawn",
    "shell:allow-kill",
    "shell:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save"
  ]
}
